{
  "name": "its-our-studio-admin",
  "private": true,
  "version": "1.0.1",
  "description": "Official Admin Dashboard for Its Our Studio",
  "author": "Its Our Studio",
  "homepage": "https://itsour-studio.vercel.app",
  "license": "UNLICENSED",
  "main": "electron/main.cjs",
  "type": "module",
  "scripts": {
    "dev": "concurrently \"npm:server\" \"npm:client\"",
    "client": "vite",
    "dev:host": "concurrently \"npm:server\" \"vite --host\"",
    "server": "node server.js",
    "build": "tsc && vite build",
    "preview": "vite preview",
    "electron:dev": "concurrently -k \"cross-env BROWSER=none npm run dev\" \"wait-on http://localhost:5173 && cross-env NODE_ENV=development electron .\"",
    "electron:build": "npm run build && electron-builder",
    "tauri:dev": "tauri dev",
    "tauri:build": "tauri build",
    "tauri": "tauri",
    "mobile:open": "npx cap open android",
    "test": "vitest",
    "test:e2e": "playwright test"
  },
  "devDependencies": {
    "@capacitor/assets": "^3.0.5",
    "@playwright/test": "^1.57.0",
    "@tauri-apps/cli": "^2.9.6",
    "@testing-library/react": "^16.3.1",
    "@types/node": "^25.0.3",
    "@types/react": "^19.2.7",
    "@types/react-dom": "^19.2.3",
    "@vitejs/plugin-react": "^5.1.1",
    "concurrently": "^9.2.1",
    "cross-env": "^10.1.0",
    "electron": "^39.2.6",
    "electron-builder": "^26.0.12",
    "jsdom": "^27.3.0",
    "png-to-ico": "^3.0.1",
    "typescript": "~5.9.3",
    "vite": "^7.2.4",
    "vitest": "^4.0.16",
    "wait-on": "^9.0.3"
  },
  "dependencies": {
    "@capacitor/android": "^8.0.0",
    "@capacitor/cli": "^8.0.0",
    "@capacitor/core": "^8.0.0",
    "bcryptjs": "^3.0.3",
    "canvas": "^3.2.1",
    "cors": "^2.8.5",
    "dotenv": "^17.2.3",
    "express": "^5.1.0",
    "express-rate-limit": "^8.2.1",
    "firebase": "^12.6.0",
    "firebase-admin": "^13.6.0",
    "lucide-react": "^0.562.0",
    "multer": "^2.0.2",
    "node-cron": "^4.2.1",
    "node-fetch": "^3.3.2",
    "nodemailer": "^7.0.11",
    "react": "^19.2.0",
    "react-colorful": "^5.6.1",
    "react-dom": "^19.2.0",
    "react-router-dom": "^7.9.6",
    "use-sound": "^5.0.0",
    "xlsx": "^0.18.5"
  },
  "build": {
    "appId": "com.itsourstudio.admin",
    "productName": "Its Our Studio Admin",
    "directories": {
      "output": "release"
    },
    "files": [
      "dist/**/*",
      "electron/**/*",
      "package.json"
    ],
    "win": {
      "target": "nsis",
      "icon": "public/logo/icon.ico"
    },
    "nsis": {
      "oneClick": false,
      "allowToChangeInstallationDirectory": true,
      "deleteAppDataOnUninstall": false
    }
  }
}