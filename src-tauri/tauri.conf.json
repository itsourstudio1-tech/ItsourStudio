{
  "$schema": "../node_modules/@tauri-apps/cli/config.schema.json",
  "productName": "Its Our Studio Admin",
  "version": "1.0.1",
  "identifier": "com.itsourstudio.admin",
  "build": {
    "frontendDist": "../dist",
    "devUrl": "http://localhost:5173",
    "beforeDevCommand": "npm run dev",
    "beforeBuildCommand": "cross-env VITE_BUILD_TARGET=tauri npm run build"
  },
  "app": {
    "windows": [
      {
        "title": "Its Our Studio Admin",
        "width": 1200,
        "height": 800,
        "maximized": true,
        "url": "index.html"
      }
    ],
    "security": {
      "csp": null
    }
  },
  "bundle": {
    "active": true,
    "targets": "all",
    "icon": [
      "icons/32x32.png",
      "icons/128x128.png",
      "icons/128x128@2x.png",
      "icons/icon.icns",
      "icons/icon.ico"
    ]
  }
}